/* vues/user/user.wxss */
.user {
    position: relative;
}

.top {
    width: 100%;
    height: 312rpx;
    background: url('https://cdn.lastmiles.cn/apps/6478452815533453312.png?size=15744');
    background-size: cover;
}

.top .img {
    width: 163rpx;
    height: 163rpx;
    border-radius: 50%;
    margin: 76rpx 0 0 42rpx
}

.top .txt {
    display: inline-block;
    margin-left: 30rpx;
    vertical-align: top;
    margin-top: 105rpx;
}

.top .txt p {
    display: block;
    margin-bottom: 35rpx;
    font-size: 33rpx;
    color: #333333;
}

.top .txt2 {
    display: inline-block;
    margin-left: 30rpx;
    vertical-align: top;
    margin-top: 123rpx;
}

.top .txt2 p {
    display: block;
    margin-bottom: 35rpx;
    font-size: 33rpx;
    color: #333333;

}

.list {
    margin-top: 20rpx;
}

.list .box {
    width: 100%;
    height: 100rpx;
    border-bottom: 1rpx solid #f0f0f0;
    position: relative;
}

.list .box view {
    display: inline-block;
    height: 100rpx;
    margin-top: -15rpx;
}

.list .box image {
    width: 25rpx;
    height: 30rpx;
    margin: 15rpx 0 0 33rpx;
}

.list .box .order {
    margin-left: 25rpx;
    font-size: 30rpx;
    margin-top: 8rpx;
    vertical-align: middle;
    line-height: 30rpx;
}

.list .box .right {
    position: absolute;
    right: 20rpx;
    top: 33rpx;
}

.list .box .right image {
    width: 36rpx;
    height: 34rpx;
}

.img {
    width: 100%;
    height: 196rpx;
    margin-top: 105rpx;
}

.bac {
    position: absolute;
    width: 596rpx;
    height: 640rpx;
    background-color: #d9bc80;
    border-radius: 10rpx;
    left: 85rpx;
    top: 200rpx;
    z-index: 9999;
    padding-top: 10rpx;
}

.bac .title {
    display: block;
    width: 100%;
    font-size: 32rpx;
    color: #ffffff;
    text-align: center;
    margin-top: 10rpx;
}

.bac-box {
    width: 556rpx;
    height: 164rpx;
    background-color: #fcdb95;
    margin: 0 auto;
    margin-top: 25rpx;
    text-align: center;
    line-height: 164rpx;
    color: #9b5f00;
    background: url('https://cdn.lastmiles.cn/apps/6481422983498838016.png?size=7735');
    background-size: cover;
}

.bac-box image {
    width: 47rpx;
    height: 37rpx;
    margin-right: 5rpx
}

.bac-list {
    width: 556rpx;
    height: 164rpx;
    background-color: #fff;
    margin: 0 auto;
    margin-top: 8rpx;
}

.bac-list-zheneg {
    width: 163rpx;
    height: 163rpx;
    text-align: center;
    padding-top: 36rpx;
    vertical-align: top;
    display: inline-block;
    margin-right: 20rpx;
}

.bac-list-zheneg-big {
    font-size: 36rpx;
    color: #803b36;
}

.bac-list-zheneg-small {
    font-size: 20rpx;
    color: #803b36;
    margin-top: 10rpx
}

.swiper {
    width: 100%;
    height: 200rpx;
}

.bac-list-fang {
    width: 357rpx;
    height: 163rpx;
    display: inline-block;
    padding-top: 30rpx;
    padding-left: 20rpx;
    box-sizing: border-box;
	position: relative;
}

.bac-list-fang-big {
    font-size: 28rpx;
    display: block;
}

.bac-list-fang-small {
    font-size: 22rpx;
}

.bac-scroll {
    width: 100%;
    height: 416rpx;
    overflow: auto;
    margin-top: 10rpx;
}

.bac-floor-box {
    width: 556rpx;
    height: 152rpx;
    margin: 0 auto;
    bottom: 0;
    left: 20rpx;
    position: absolute;
}

.bac-floor-box-img {
    width: 100%;
    height: 100%;
}

.bac-floor-box-btn button {
    width: 288rpx;
    height: 62rpx;
    background-color: #3f361e;
    border-radius: 10rpx;
    line-height: 62rpx;
    position: absolute;
    top: 46rpx;
    left: 147rpx;
    font-size: 28rpx;
    color: #fff;
}

.tenant-model {
    position: absolute;
    width: 100%;
    height: 100%;
    position: fixed;
    left: 0;
    top: 0;
    z-index: 1000;
    background-color: rgba(0, 0, 0, 0.5);
}
.bac-list-fang-num{
	position: absolute;
	right: 10rpx;
	top: 53rpx;
	font-size: 24rpx;
}