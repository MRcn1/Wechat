/* pages/tick/tick.wxss */

.top{
  width: 90%;
  padding-top: 100rpx;
  margin: 0 auto;
}
.top .text{
  font-size: 28rpx;
  display: inline-block;
  width: 100%;
  text-align: center;
}


.btn{
  width: 100%;
  margin-top: 140rpx;
}

.btn .view{
  width: 78%;
  background: url('http://oss.life-go.cn/apps/6400613975632924672.png') no-repeat;
  background-size: 100% 100%;
  height: 120rpx;
  text-align: center;
  line-height: 100rpx;
  position: relative;
  margin: 0 auto;
  letter-spacing: 5rpx;
  font-size: 34rpx;
}

.btn .cannel{
  width: 73%;
  height: 86rpx;
  text-align: center;
  line-height: 86rpx;
  position: relative;
  margin: 0 auto;
  letter-spacing: 5rpx;
  font-size: 34rpx;
  border:1px solid #ffc2a1;
  border-radius: 20rpx;
}
