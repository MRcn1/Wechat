/* vues/voucher/voucher.wxss */
.voucher-header{
    width: 100%;
    height: 310rpx;
    background-color: #f5f5f5;
    padding: 25rpx 35rpx;
    box-sizing: border-box;
}
.voucher-header-img{
    width: 680rpx;
	height: 263rpx;
	background-color: #ffffff;
}
.voucher-cont{
    position: relative;
    width: 750rpx;
	height: 883rpx;
	background-color: #ffffff;
}
.voucher-cont-list{
    width: 100%;
    padding: 21rpx;
    font-size: 26rpx;
    color: #999999;
}
.voucher-cont-title{
    display: inline-block;
    width: 130rpx;
    height: 24rpx;
    color: #333333;
    vertical-align: top;
}
.voucher-cont-txt{
    display: inline-block;
    width: 534rpx;
    vertical-align: top;
}
.voucher-btn{
    width: 594rpx;
	height: 80rpx;
	background-color: #803b36;
    border-radius: 10rpx;
    position: absolute;
    bottom: 131rpx;
    left: 78rpx;
}
.voucher-btn button{
    background-color: #803b36;
    color: #ffffff;
}
.result-txt{
    width: 214rpx;
    height: 201rpx;
    margin: 271rpx 268rpx 0 268rpx;
}
.result-btn{
    width: 594rpx;
    height: 80rpx;
    font-size: 30rpx;
    margin-bottom: 32rpx
}
.center {
  padding: 15rpx 0rpx;
  position: relative;
}

.center .card {
  height: 243rpx;
  background-color: #fff;
  position: relative;
  z-index: 10;
}

.center .card .leftImg {
  width: 205rpx;
  height: 205rpx;
  border-radius: 10rpx;
  margin-top: 20rpx;
  margin-left: 20rpx;
  float: left;
}

.center .card .centerBox {
  float: left;
  margin-left: 20rpx;
}

.center .card .centerBox .title {
  color: #333;
  font-size: 32rpx;
  width: 300rpx;
  display: inline-block;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  margin-top: 30rpx;
}

.center .topBox {
  position: absolute;
  right: 20rpx;
  top: 10rpx;
  width: 87rpx;
  height: 99rpx;
  background: url('https://cdn.lastmiles.cn/apps/6480684759344230400.png?size=1980') no-repeat;
  background-size: 100% 100%;
  text-align: center;
  z-index: 20;
  color: #fff;
}

.center .bottoms {
  position: absolute;
  bottom: 0;
  right: 50rpx;
}

.center .bottoms text {
  display: inline;
  font-size: 26rpx;
  color: #333;
  border-bottom: 1px solid #333;
}

.center .bottoms .right {
  margin-left: 70rpx;
  color: #803b36;
  border-bottom: 1px solid #803b36;
}

.center text {
  font-size: 28rpx;
  display: block;
  line-height: 35rpx;
}

.center .big {
  font-size: 36rpx;
  width: 100%;
  margin-top: 10rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.center .bottomImg {
  height: 120rpx;
  position: absolute;
  bottom: 6rpx;
  width: 100%;
  z-index: 1;
}

.center .money {
  background: url(https://cdn.lastmiles.cn/apps/6480693444527271936.png?size=7067) no-repeat;
  background-size: 100% 100%;
  width: 205rpx;
  height: 205rpx;
  border-radius: 10rpx;
  margin-top: 20rpx;
  margin-left: 20rpx;
  float: left;
}

.center .money text{
  line-height: 205rpx;
  text-align: center;
  display: inline-block;
  width: 100%;
  font-size: 48rpx;
  color: #fff;
}

.duihuan{
  width: 100%;
  text-align: center;
  font-size: 30rpx;
  margin-bottom: 268rpx;
  margin-top: 10rpx;
}