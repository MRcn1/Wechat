/* vues/myKaBao/myKaBao.wxss */

page {
    background-color: #f5f5f5;
}

.myKaBao-top {
    width: 100%;
    height: 80rpx;
    background-color: #fff;
    display: flex;
    font-size: 26rpx;
    color: #666;
    line-height: 80rpx;
    justify-content: space-around;
}

.active {
    position: relative;
}
.active0{
    margin-bottom: 125rpx;
    height: 970rpx;
    overflow: auto;
    margin-top: 10rpx;
}
.active:after {
    width: 100%;
    content: ' ';
    height: 3px;
    background-color: #803b36;
    position: absolute;
    bottom: 0;
    left: 0;
}

.coupon-selset {
    width: 100%;
    height: 54rpx;
    background-color: #feede3;
    font-size: 22rpx;
    color: #666;
    line-height: 54rpx;
    padding: 5rpx 15rpx;
    box-sizing: border-box;
}

.coupon-list {
    width: 710rpx;
    height: 200rpx;
    background-color: #f5f5f5;
    margin: 0 auto;
    margin-top: 20rpx;
    position: relative;
    overflow: hidden;
}

.coupon-list-zheng {
    width: 200rpx;
    height: 200rpx;
    display: inline-block;
    text-align: center;
    overflow: hidden;
    background-image: url('https://cdn.lastmiles.cn/apps/6482913771521060864.png?size=1622');
    background-size: cover;
    vertical-align: top;
}

.coupon-list-fang {
    width: 500rpx;
    height: 200rpx;
    display: inline-block;
    overflow: hidden;
    padding: 30rpx 10rpx 0 20rpx;
    box-sizing: border-box;
    background-image: url('https://cdn.lastmiles.cn/apps/6482914027004506112.png?size=1918');
    background-size: cover;
}
.coupon-list-fang-middle{
    line-height: 127rpx;
}

.coupon-list-big {
    color: #fff;
    font-size: 60rpx;
    display: block;
    margin-top: 31rpx;
}

.coupon-list-small {
    color: #fff;
    display: block;
    font-size: 20rpx;
	margin-top: 10rpx;
}

.coupon-list-fang-big {
    color: #333;
    font-size: 28rpx;
    display: block;
}

.coupon-list-fang-small {
    color: #333;
    display: block;
    font-size: 22rpx;
}

.coupon-list-fang-btn {
    font-size: 22rpx;
    color: #803b36;
    position: absolute;
    right: 40rpx;
    bottom: 10rpx;
}
.coupon-list-fang-img{
    width: 30rpx;
    height: 30rpx;
    position: absolute;
    right: 40rpx;
    bottom: 100rpx;
}

.coupon-list-fang-btn text {
    display: inline-block;
    width: 134rpx;
    height: 48rpx;
    border-radius: 24rpx;
    border: solid 1rpx #803b36;
    text-align: center;
    line-height: 48rpx;
    margin-left: 20rpx;
}

.coupon-list-img {
    width: 30rpx;
    height: 30rpx;
    position: absolute;
    top: 37rpx;
    right: 45rpx;
}
.coupon-list-image{
	width: 100%;
	height: 100%;
}
/* 
.coupon-list-img image {
    width: 123rpx;
    height: 93rpx;
} */

.coupon-btn {
    margin-left: 77rpx;
    position: fixed;
    bottom: 20rpx;
    height:100rpx;
}

.coupon-btn button {
    width: 594rpx;
    background-color: #fff;
    border-radius: 10rpx;
    color: #000;
    border: solid 1rpx #ccc;
}

.coupon-list-zheng .img {
    width: 100%;
    height: 100%;
}
.history .coupon-list-fang-btn2 {
    font-size: 22rpx;
    position: absolute;
    right: 30rpx;
    bottom: 10rpx;
    width: 134rpx;
    height: 48rpx;
    border-radius: 24rpx;
    color: #d9bc80;
	border: solid 1px #d9bc80;
	text-align: center;
	line-height: 48rpx;
}
