/* vues/personal/personal.wxss */
.list{
    margin-top: 10rpx;
}
.list .box{
    width: 100%;
    height: 100rpx;
    border-bottom: 1rpx solid #f0f0f0;
    position: relative;
}
.list .box .name{
    height: 100rpx;
}

.list .box image{
    width: 25rpx;
    height: 30rpx;
    margin: 15rpx 0 0 33rpx;
    vertical-align: top;
}
.list .box .order{
    margin-left: 25rpx;
    font-size: 30rpx;
}
.list .box .right{
    position: absolute;
    right: 20rpx;
    top: 20rpx;
}
.list .box .right image{
    width: 36rpx;
	height: 34rpx;
}
.list .box .right .huodong{
    width: 74rpx;
    height: 74rpx;
    margin-top: -10rpx;
}
.list .box .right .txt{
    font-size: 30rpx;
    color: #888888;
    text-align: center;
	vertical-align: bottom;
}
.list .box .right .txt2{
	position: absolute;
    font-size: 30rpx;
    color: #888888;
    right: 50rpx;
	top: 15rpx;
}
.dropOut{
    width: 594rpx;
	height: 88rpx;
	border-radius: 10rpx;
    margin:0 auto;
    margin-top:150rpx ;
}
.dropOut button{
    background-color: #fff;
    border: solid 2rpx #803b36;
}