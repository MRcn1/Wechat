  /* margin-top: 122rpx; */
}

.left .li {
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  font-size: 26rpx;
  color: #888;
}
.old {
  color: #b0b0b0;
  font-size: 24rpx;
  text-decoration: line-through;
  margin-left: 20rpx; 
}

.left .li.active {
  background: white;
  color: #333;
}

.right {
  
}

page {
  height: 100%;
}

.scr {
  position: fixed;
  left: 25%;
  top: 0;
  width: 75%;
  height: 91%;
  /* margin-top: 123rpx; */
  /* width: 75%;
    height: 100%; */
}

.bar {
  height: 100rpx;
  font-size: 28rpx;
  color: #5c5c5c;
  line-height: 101rpx;
  margin-left: 5%;
}

.contants {
  width: 100%;
  /* height: 200rpx; */
  zoom: 1;
  overflow: auto;
  height: 190rpx;
}

.contants image {
  width: 30%;
  border: 1px solid #eee;
  margin-left: 5%;
  float: left;
  margin-right: 5%;
  height: auto;
}

.contants .rights {
  width: 55%;
  float: left;
}
.overauto{
  height: 415rpx;
  overflow: auto;
}

.contants .rights .text {
  font-size: 28rpx;
  color: #666;
  display: inline-block;
  height: 80rpx;
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}

.contants .bottoms {
  margin-top: 20rpx;
  zoom: 1;
  overflow: auto;
}

.contants .bottoms .money {
  color: #ff673e;
  font-size: 25rpx;
  height: 80rpx;
  line-height: 80rpx;
  float: left;
}

.contants .bottoms .moneyBig {
  font-size: 30rpx;
}

.contants .bottoms .mark {
  /* min-width: 150rpx; */
  float: right;
  height: 80rpx;
  max-width: 200rpx;
  /* line-height: 80rpx; */
}

.pluss {
  display: inline-block;
  /* padding: 20rpx; */
  background-color: #ffc2a1;
  /* line-height: 80rpx;  */
  height: 40rpx;
  width: 40rpx;
  text-align: center;
  margin-top: 20rpx;
  line-height: 33rpx;
  border-radius: 180rpx;
}

.minus {
  display: inline-block;
  /* padding: 20rpx; *//* background-color: #ffc2a1; *//* line-height: 80rpx;  */
  height: 40rpx;
  width: 40rpx;
  text-align: center;
  margin-top: 20rpx;
  line-height: 33rpx;
  border-radius: 180rpx;
  border: 1px solid #888;
}

.mark .number {
  font-size: 30rpx;
  color: #666;
  min-width: 35rpx;
  display: inline-block;
  text-align: center;
  margin: 0 18rpx;
}

.bottomBox {
  position: fixed;
  width: 100%;
  height: 120rpx;
  bottom: 0;
  left: 0;
  z-index: 999;
}

.bottomBox .leftBox {
  width: 70%;
  background: #3a322d;
  height: 100rpx;
  float: left;
  margin-top: 20rpx;
  color: white;
  text-align: center;
}

.bottomBox .rightBox {
  width: 30%;
  background: #ffc2a1;
  height: 100rpx;
  float: left;
  margin-top: 20rpx;
  color: #333;
  text-align: center;
}

.bottomBox .rightBox text {
  display: inline-block;
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 30rpx;
}

.bottomBox .leftBox .ttee {
  display: inline-block;
  width: 70%;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 30rpx;
  margin-left: 30%;
  text-align: left;
}

.leftBox .leftImgbox {
  position: absolute;
  left: 30rpx;
  bottom: 20rpx;
}

.leftBox .img {
  width: 100rpx;
  height: 100rpx;
}

.leftBox .leftImgbox text {
  font-size: 18rpx;
  display: inline-block;
  color: white;
  background-color: #e53417;
  padding: 5rpx 7rpx;
  border-radius: 180rpx;
  text-align: center;
  min-width: 20rpx;
  position: absolute;
  top: 0;
  right: 0rpx;
}

.rule {
  position: fixed;
  width: 100%;
  height: 101%;
  background: rgba(0, 0, 0, 0.3);
  z-index: 9999;
  margin-top: -10rpx;
}

.ruleBox {
  width: 85%;
  /* height: 200rpx; */
  position: relative;
  margin: 0 auto;
  background: rgb(255, 255, 255);
  margin-top: 20%;
  border-radius: 20rpx;
  overflow: hidden;
}
.ruleBox image{
  width: 100%;
  height: 100%;
}
.ruleBox .img{
  width: 100%;
	height: 318rpx;
}
.ruleBox .title {
  /* text-align: center; */
  width: 100%;
   height: 100%; 
  /* line-height: 80rpx; */
