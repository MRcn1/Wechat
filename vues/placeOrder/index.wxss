  margin-top: 20rpx;
  margin-left: 20rpx;
  font-size: 32rpx;
  /* position: relative; */
  color: #333;
}
.product-description{
  width: 90%;
  font-size: 22rpx;
  color: #999999;
  padding-left: 31rpx;
}
.product-description-title{
  display: inline-block;
  vertical-align: top;
}
.product-description-name{
  display: inline-block;
  width: 367rpx;
	height: 52rpx;
}
.ruleBox .title image {
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  right: 55rpx;
  top: 42rpx;
}

