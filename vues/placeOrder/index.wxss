}

.ruleBox .btnGroup .btnBox text {
  /* min-width: 25%; */
  display: inline-block;
  height: 60rpx;
  border: 1px solid #bbb;
  font-size: 25rpx;
  text-align: center;
  line-height: 60rpx;
  margin-bottom: 20rpx;
  margin-left: 20rpx;
  border-radius: 10rpx;
  padding: 0 20rpx;
  color: #666;
}

.rulemoney {
  height: 80rpx;
  margin-top: 20rpx;
  width: 100%;
  zoom: 1;
  overflow: auto;
  padding-bottom: 20rpx;
  line-height: 70rpx;
}

.rulemoney .textleft {
  margin-left: 20rpx;
  font-size: 22rpx;
  color: #ff673e;
  display: inline-block;
}

.rulemoney .textright {
  display: inline-block;
  width: 250rpx;
  font-size: 25rpx;
  text-align: center;
  background-color: #ffc2a1;
  border-radius: 100rpx;
